{
    "name": "sclaravel/menus",
    "description": "Laravel Menus",
    "keywords": ["laravel", "menu", "navigation", "nav"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/sclaravel/menus/issues",
        "source": "https://github.com/sclaravel/menus"
    },
    "authors": [
        {
            "name": "Truong Thai Son",
            "email": "sontruong2509@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-json": "*",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "TysonLaravel\\Menus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TysonLaravel\\Menus\\MenusServiceProvider"
            ],
            "aliases": {
                "Menu": "TysonLaravel\\Menus\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "stable"
}
